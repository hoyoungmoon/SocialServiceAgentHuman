<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.realproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/admob_app_id" />

        <activity
            android:name=".Splash_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Main_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Manual_Activity"
            android:label="휴가 및 월급 계산 방식"
            android:parentActivityName=".Main_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/ManualTheme" />
        <activity
            android:name=".Calculator_Activity"
            android:label="월급 계산기"
            android:parentActivityName=".Main_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/CalculatorTheme" />
        <activity
            android:name=".Setting_Activity"
            android:label="설정하기"
            android:parentActivityName=".Main_Activity"
            android:screenOrientation="portrait"
            android:theme="@style/CalculatorTheme" />
    </application>

</manifest>